# Database connection string (set to SQLite for local or Postgres in prod)
DATABASE_URL=

# JWT secrets (required)
JWT_ACCESS_SECRET=
JWT_REFRESH_SECRET=

# JWT expirations
JWT_ACCESS_EXPIRES_IN="15m"
JWT_REFRESH_EXPIRES_IN="30d"

# Server
PORT=3000

# CORS origin (set to your client origin, e.g., http://localhost:3000)
CORS_ORIGIN=
